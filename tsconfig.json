{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~src/*": [
                "src/*"
            ]
        },
        "target": "es5",
        "strict": true,
        "jsx": "react",
        "noEmit": true,
        "lib": [
            "webworker",
            "dom"
        ],
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false
    },
    "exclude": ["node_modules/"]
}